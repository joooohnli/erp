<form id="priceAlarm" class="x-form" method="POST" action="batNumberLength_change.jsp">
<input type="hidden" name="stock_name" value="<%=exchange.toHtml(stock_name)%>">
 <table <%=TABLE_STYLE2%> class="TABLE_STYLE2">
 <tr <%=TR_STYLE1%> class="TR_STYLE1">
 <td <%=TD_STYLE3%> class="TD_STYLE3"><div <%=DIV_STYLE1%> class="DIV_STYLE1"><input type="submit" <%=SUBMIT_STYLE1%> class="SUBMIT_STYLE1" value= "<%=demo.getLang("erp","设置")%>"></div></td>
 </tr>
 </table>
