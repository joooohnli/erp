<?xml version="1.0" encoding="utf-8"?>
<tree-config css="../../../css/include/nseer_cookie/xml-css.css">
<table display-name="全部机构" name="hr_config_file_kind" tree-name="Nseer_tree1" div-name="nseer1"
		add-brother-node="div-add1.xml,div-add2.xml" 
		add-child-node="div-add1.xml,div-add2.xml" 
		delete-node="div-delete.xml"
		change-node="div-change1.xml,div-change2.xml"
		search-node="init_quickSearchView.xml"
		step-length="2"
		first-length="2">
		<field display-name="序号" name="id">
		</field>
		<field display-name="节点ID" name="CATEGORY_ID" depend="required">
		</field>
		<field display-name="父节点ID" name="PARENT_CATEGORY_ID" depend="required">
		</field>
        <field display-name="节点显示文字" name="CATEGORY_NAME" depend="required">
		</field>
        <field display-name="是否有下级" name="DETAILS_TAG" depend="required">
		</field>
        <field display-name="机构编号" name="FILE_ID" depend="required">
		<depend name="required" />
		<depend name="duplicate" param0="hr_config_file_kind@file_id@parent_category_id"/>
		<depend name="stepNum" param0="2"/>
		</field>
        <field display-name="机构名称" name="FILE_NAME" depend="required">
        <depend name="common"/>
		<depend name="required" />
		<depend name="duplicate" param0="hr_config_file_kind@file_name@parent_category_id@file_id"/>
		</field>
		<field display-name="是否可删除" name="DELETE_TAG" depend="required">
		</field><!--以上七个字段为tree必须，名称可变上下顺序不能变-->
        <field display-name="机构简称" name="NICK_NAME" depend="nodeAttribute">
        <depend name="common"/>
		</field>
        <field display-name="ID串" name="CHAIN_ID" depend="nodeAttribute">
		</field>
        <field display-name="name串" name="CHAIN_NAME" depend="nodeAttribute">
		</field>
        <field display-name="是否为零售店" name="DESCRIBE3" depend="nodeAttribute">
		</field>
        <field display-name="责任人编号" name="DESCRIBE1" depend="nodeAttribute">
        <depend name="chief" param0="hr_config_human_id_temp"/>
		</field>
</table>
</tree-config>